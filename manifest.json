{
  "manifest_version": 3,
  "name": "Quick Query",
  "version": "1.3",
  "description": "Quickly navigate to websites using URL Templates.",
  "permissions": ["storage"],
  "background": {
    "service_worker": "js/background.js"
  },
  "omnibox": {
    "keyword": "q"
  },
  "action": {
    "default_popup": "public/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_ui": {
    "page": "public/onboarding.html"
  }
}
